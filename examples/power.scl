-include "io.scl"

fn main() : int {
  int base
  int exponent
  int result = 1
  int counter = 0
  
  printf("Enter base and exponent: ")
  scanf("%d%d", &base, &exponent)
  
  if exponent == 0 then goto :end
  
  while counter < exponent {
    result = result * base
    counter = counter + 1
  }
  
  :end
  printf("%d\n", result)
  
  return 0
}
