-include "io.scl"

fn main() : int {
  int n

  printf("Enter a number: ")
  scanf("%d", &n)
  
  if n == 0 printf("0\n") 
  if n == 1 printf("1\n") 
  
  int a = 0
  int b = 1
  int count = 2
  int fib = 0
  
  while count < n {
    fib = a + b
    a = b
    b = fib
    count = count + 1
  }
  
  printf("%dth fibobacci number: %d\n", n, fib)
  return 0
}
